<script src="js/jquery-3.3.1.min.js"></script>
<script src="js/jquery.cycle.js"></script>
<script>
	
	$("#next").click(function(){
		$(".cycle-slideshow").cycle('next');
	});
	$("#prev").click(function(){
		$(".cycle-slideshow").cycle('prev');
	});

	$(".slot2handle").click(function(){
		$(".slot2").fadeIn();
		$(".slot1").fadeOut();
	});
	$(".slot1handle").click(function(){
		$(".slot1").fadeIn();
		$(".slot2").fadeOut();
	});

</script>
